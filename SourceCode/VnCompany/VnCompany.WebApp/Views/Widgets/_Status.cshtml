@using VnCompany.WebApp.Resources
@model int

@if (Model == 1)
{
    <span class="m-badge m-badge--wide m-badge--success"><i class="fa fa-check"></i> @ManagerResource.LB_ACTIVATED</span>
}
else if (Model == 0)
{
    <span class="m-badge m-badge--wide m-badge--danger"><i class="fa fa-lock"></i> @ManagerResource.LB_LOCKED</span>
}